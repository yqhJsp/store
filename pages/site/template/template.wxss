/*列表*/

.signOne {
  margin-top: 20rpx;
  background: #fff;
  border-radius: 16rpx;
  position: relative;
  box-shadow: #dedede 0px 0px 8px;
  display: flex;
  flex-flow: row;
  padding: 0 40rpx;
  height: 212rpx;
}

.signImg {
  width: 133rpx;
  text-align: center;
  padding: 40rpx 0;
}

.signImg image {
  width: 133rpx;
  height: 133rpx;
  border-radius: 16rpx;
}

.signCenter {
  width: 80%;
  padding: 35rpx 20rpx;
}

.signCenter .toptitle {
  font-size: 34rpx;
  color: #333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.signDate {
  font-size: 24rpx;
  color: #999;
  margin-top: 10rpx;
}

.signNum {
  color: #29f;
  font-size: 24rpx;
  margin-top: 20rpx;
}

.signicon {
  height: 28rpx;
  width: 26rpx;
  display: inline-block;
  margin-right: 10rpx;
}

.num {
  vertical-align: 5rpx;
}

.signBtn {
  width: 108rpx;
  height: 44rpx;
  line-height: 44rpx;
  font-size: 24rpx;
  color: #fff;
  position: absolute;
  bottom: 25rpx;
  right: 40rpx;
  background: #29f;
  border-radius: 8rpx;
  text-align: center;
}

.signActive {
  background: #ccc;
}

.tab-content1 {
  height: 100%;
  align-items: center;
  vertical-align: middle;
  text-align: center;
}

.tab-image {
  margin-top: 130rpx;
  margin-bottom: 50rpx;
  width: 252rpx;
}

