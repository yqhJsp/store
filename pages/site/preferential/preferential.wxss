/* pages/site/preferential/preferential.wxss */
@import '../template/template.wxss';
page{
  background: #f0f0f0
}
.session{
  margin-bottom:50rpx
}
.pretable{
  padding:0 30rpx;      
  font-size:28rpx;
}
.preTitle{
font-size:28rpx;
padding:30rpx 0 15rpx 0;
}
.pretr,.distr{
  font-size:28rpx;
  background-color: #fff; 
  box-shadow: #dedede 0px 0px 8px; 
  border-radius:8rpx
}
.pretr{
margin-top:10rpx;  
height:90rpx;
line-height:90rpx;
display: flex;
flex-flow: row;
padding:0 20rpx;
}
.distr{
 margin-top:20rpx;  
}
.distr .distd{
padding:0 20rpx;
height:90rpx; 
background-color: #fff; 
line-height:90rpx;
display: flex;
flex-flow: row;
  border-radius:8rpx
}
.boder{
 border-bottom: #dedede 1rpx solid;  
}
.prerow{
width:40%;
}
.prerow2,.prerow3{
text-align:right
}
.prerow2{
width:60%;
}
.prerow3{
  width:85%;
}
.cotip {
  width: 5%;
  text-align: right;
}
.itemImage {
  width: 20rpx;
  height: 25rpx;
  margin-top: 20rpx;
}
.preInput{
 height:90rpx;
 line-height:90rpx;
 border: 0 
}
.integleft {
  width: 30%;
}
.integt{
  width:17%
}
.integInput {
  border: none;
  display: inline-block;
  line-height: 90rpx;
  height:90rpx;
   text-align: right;
   padding-left: 20rpx
}
.noprefent{
 margin:40rpx 0 20rpx 0;
 font-size:28rpx; 
}
.integright {
  width: 55%;
  font-size: 24rpx;
}
.pression{
  margin-top:60rpx;
}
.prebottom{
margin:30rpx 0 0 0;
}
.preTip{
font-size:30rpx;
}
.prices{
  color: red
}
/*优惠卷弹窗*/
.commodity_screen {
  background-color: rgba(0, 0, 0, 0.7);
  width: 100%;
  height: 100%;
  opacity: 0.5;
  position: fixed;
  z-index: 666;
  bottom: 0;
}
.coupon_area{
  min-height: 600rpx;
  max-height: 800rpx;
  position: fixed;
  z-index: 888;
  bottom:0;
  width:100%;
}
.couponOne{
  margin-top:20rpx
}
.coupon_area{
background-color:#f5f5f5
}
.display-block{
  display:block !important;
}
.display-none{
  display: none !important;
}
   .couponTitle{
     position:fixed;
     top:0;
     width: 100%;
     padding:20rpx 0;
   }
   .coupon_tip{
     font-size:30rpx;
     text-align:center
   }

.coupon_no{
   text-align: center
}
.couponClose{
  width: 30rpx;
  height:30rpx;
  position: absolute;
  top:0;
  right:0;
}
.couponClose{
  padding:20rpx;
}
.coupon_no{
   text-align: center;
}
.coupon_no image{
  width: 165rpx;
  height: 179rpx;
    margin-top:60rpx
}
.couponDome{
height:1000rpx;
margin-top:100rpx;
overflow: auto
}
.disabled { 
pointer-events: none;
background:#dedede;
color:#333
}
